(defpclass SituatedObject []
  :meta {:doc "Where the entity is located"}
  :fields {x 0
           y 0
           z 0
           motion_x 0
           motion_y 0
           motion_z 0
           yaw 0
           pitch 0
           space {:observable false}})

(defpclass RectangularVolume [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name]
  :fields {tl-x topLeftX  
           tl-y topLeftZ
           tl-z topLeftY
           br-x bottomRightX
           br-y bottomRightZ
           br-z bottomRightY
           v-name name})

