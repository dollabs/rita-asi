(defpclass Entrance-Commands [] :modes [:enter :leave :lock :unlock :open :close])

(defpclass Portal [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name
                   cx]
  :inherit [RectangularVolume]
  :fields {connection cx}               ; shared lvar representing the connection
  :modes [:open])

(defpclass hole-in-the-wall [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name cx]
  :inherit [Portal]
  :fields {connection cx}               ; shared lvar representing the connection
  :modes [:open])

(defpclass blockage  [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name
                 cx]
  :meta {:doc "A Blockage"}
  :inherit [Portal]
  :modes [:blocked :unblocked])

(defpclass Door-commands [] :modes [:open :close :lock :unlock :leave :enter])

(defpclass Door [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name
                 cx]
  :meta {:doc "A Door"}
  :modes [:open :closed :locked :blocked]
  :inherit [Portal]
  :methods [(defpmethod open
              {:pre (and :closed (= command (mode-of Door-commands :open)))
               :post :open
               :doc "opens the door"}
              [command])
            (defpmethod close
              {:pre (and :open (= command (mode-of Door-commands :close)))
               :post :closed
               :doc "closes the door"}
              [command])
            (defpmethod enter
              {:pre (and :open (or (= command (mode-of Door-commands :leave))
                                   (= command (mode-of Door-commands :enter))))
               :doc "Enters the doorway"}
              [command])
            (defpmethod lock
              {:pre (and :closed (= command (mode-of Door-commands :lock)))
               :post :locked
               :doc "locks the door"}
              [command])
            (defpmethod unlock
              {:pre (and :locked (= command (mode-of Door-commands :unlock)))
               :post :closed
               :doc "unlocks the door"}
              [command])])

(defpclass DoubleDoor [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name
                       cx]
  :inherit [Door])

(defpclass DoorlessEntrance [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name
                             cx]
  :meta {:doc "A walk-in entrance without a door"}
  :inherit [Portal]
  :methods [(defpmethod walk-into-corridor
              {:pre (or (= command (mode-of Door-commands :enter))
                        (= command (mode-of Door-commands :leave)))
               :doc "Enter through a doorless entrance"}
              [command])])

(defpclass CorridorJoin [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name
                         cx]
  :meta {:doc "A Corridor Join, like a door for corridoors"}
  :inherit [Portal]
  :methods [(defpmethod walk-into-corridor
              {:pre (or (= command (mode-of Door-commands :enter))
                        (= command (mode-of Door-commands :leave)))
               :doc "passes from one corridor into another"}
              [command])])


(defpclass Victim-commands [] :modes [:triage])

