;; RectangularVolume is used to create any objects that has a rectangular shape: rooms, corridors.
(defpclass RectangularVolume [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name]
  :fields {tl-x topLeftX  
           tl-y topLeftZ
           tl-z topLeftY
           br-x bottomRightX
           br-y bottomRightZ
           br-z bottomRightY
           v-name name})

;; General, DoorCommands and Portal are used to create Door-related objects
(defpclass General [] :modes [:initial :nothing])
(defpclass DoorCommands [] :modes [:open :close :lock :unlock :leave :enter])
(defpclass Portal [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY name
                   connId]
  :inherit [RectangularVolume]
  :fields {connection connId}
  :modes [:open])

