;;----------------------------------------------------;;
;;                Unused Functions                    ;;
;;----------------------------------------------------;;
(defpclass Entrance-Commands [] :modes [:enter :leave :lock :unlock :open :close])
(defpclass Illumination [] :modes [:dark :light])
(defpclass SituatedObject []
  :meta {:doc "Where the entity is located"}
  :fields {x 0
           y 0
           z 0
           motion_x 0
           motion_y 0
           motion_z 0
           yaw 0
           pitch 0
           space {:observable false}})

(defpclass DoorlessEntrance [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY
                            name connId]
  :meta {:doc "A walk-in entrance without a door"}
  :inherit [Portal]
  :methods [(defpmethod walk-into-corridor
              {:pre (or (= command (mode-of DoorCommands :enter))
                        (= command (mode-of DoorCommands :leave)))
               :doc "Enter through a doorless entrance"}
              [command])])

(defpclass CorridorJoin [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY
                        name connId]
  :meta {:doc "A Corridor Join, like a door for corridoors"}
  :inherit [Portal]
  :methods [(defpmethod walk-into-corridor
              {:pre (or (= command (mode-of DoorCommands :enter))
                        (= command (mode-of DoorCommands :leave)))
               :doc "passes from one corridor into another"}
              [command])])

(defpclass Light  [topLeftX topLeftZ topLeftY bottomRightX bottomRightZ bottomRightY
                  name connId]
  :inherit [RectangularVolume]
  :modes [:dark :light :failed]
  :fields {connections connId})
