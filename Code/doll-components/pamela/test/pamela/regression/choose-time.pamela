(defpclass choose-time []
  :meta {:version "0.2.0"
         :doc "choose example with time bounds"}
  :methods [(defpmethod example
              {:doc "choose based on time"}
              []
              (choose
                (choice :bounds [1 100] (delay))
                (choice :bounds [101 200] (delay))
                (choice :bounds [201 300] (delay))))])
